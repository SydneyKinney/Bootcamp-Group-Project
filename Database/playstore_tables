-- Create tables for google-playstore
CREATE TABLE google_app_info (
    app_name VARCHAR,
    category VARCHAR,
    rating FLOAT,
    rating_count FLOAT,
    free BOOL,
    price FLOAT,
    content_rating VARCHAR,
    editors_choice BOOL,
    PRIMARY KEY (app_name)   
);
    
 
CREATE TABLE app_installs (
    app_name VARCHAR,
    installs FLOAT,
    minimum_installs FLOAT,
    maximum_installs FLOAT,
    size FLOAT,
    ad_supported BOOL,
    in_app_purchases BOOL,
    PRIMARY KEY (app_name)
);

-- Merge Tables
SELECT *
INTO playstore 
FROM google_app_info 
JOIN app_installs
USING (app_name);

SELECT * FROM playstore;